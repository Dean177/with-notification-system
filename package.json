{
  "author": "Dean Merchant",
  "name": "with-notification-system",
  "version": "0.9.0",
  "dependencies": {
    "@types/react-notification-system": "0.2.35",
    "prop-types": "^15.6.0",
    "react-notification-system": "0.2.16"
  },
  "peerDependencies": {
    "react": "15.6.x || 16.x",
    "react-dom": "15.6.x || 16.x"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.3",
    "@types/jest": "21.1.5",
    "@types/lodash": "4.14.81",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.20",
    "@types/react-dom": "16.0.2",
    "concurrently": "3.5.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.3",
    "jest": "21.2.1",
    "jsdom": "11.3.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-test-renderer": "16.0.0",
    "ts-jest": "21.1.4",
    "tslint": "5.8.0",
    "typescript": "2.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTestFramework.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/withNotificationSystem.js",
  "types": "dist/withNotificationSystem.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "tsc --pretty --watch",
    "build:docs": "rm -r ./docs && cd example && yarn && yarn build && cp -r ./build/ ../docs",
    "lint": "tslint src/**/*.tsx examples/src/**",
    "prepublish": "yarn test && yarn build && yarn build:docs",
    "start": "concurrently -n \"build,test\" \"npm run build:dev\" \"npm run test:dev\"",
    "test": "jest --coverage",
    "test:dev": "jest --watch"
  }
}
