{
  "author": "Dean Merchant",
  "name": "with-notification-system",
  "version": "0.9.2",
  "dependencies": {
    "@types/prop-types": "15.5.2",
    "@types/react-notification-system": "0.2.35",
    "prop-types": "^15.6.0",
    "react-notification-system": "0.2.16"
  },
  "peerDependencies": {
    "react": "15.6.x || 16.x",
    "react-dom": "15.6.x || 16.x"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.5",
    "@types/enzyme": "3.1.5",
    "@types/jest": "21.1.6",
    "@types/react": "16.0.21",
    "@types/react-dom": "16.0.3",
    "concurrently": "3.5.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "21.2.1",
    "jsdom": "11.4.0",
    "react": "16.2.0",
    "react-dom": "16.0.0",
    "react-test-renderer": "16.0.0",
    "ts-jest": "21.2.3",
    "tslint": "5.8.0",
    "typescript": "2.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTestFramework.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/withNotificationSystem.js",
  "types": "dist/withNotificationSystem.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "tsc --pretty --watch",
    "build:docs": "rm -rf ./docs && cd example && yarn && rm -rf ./node_modules/with-notification-system/node_modules/react && yarn build && cp -r ./build ../docs",
    "lint": "tslint src/**/*.tsx examples/src/**",
    "prepublish": "yarn build && yarn build:docs && yarn test",
    "start": "concurrently -n \"build,test\" \"yarn build:dev\" \"yarn test:dev\"",
    "test": "jest --coverage",
    "test:dev": "jest --watch"
  }
}
